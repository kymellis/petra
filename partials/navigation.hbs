<nav class="c-navigation">
  <ul class="c-navigation__primary">
    {{#get "navigation" label="main"}}
      {{#foreach navigation}}
        <li><a href="{{url}}">{{label}}</a></li>
      {{/foreach}}
    {{/get}}
    {{#if @member}}
      <!-- Display link to premium membership if user is logged in -->
      <li><a href="/premium-membership">Premium Membership</a></li>
    {{else}}
      <!-- Display signup and signin links if user is not logged in -->
      <li><a href="/signup">Signup</a></li>
      <li><a href="/signin">Signin</a></li>
    {{/if}}
  </ul>
  <ul class="c-navigation__secondary">
    {{#get "navigation" label="secondary"}}
      {{#foreach navigation}}
        <li><a href="{{url}}">{{label}}</a></li>
      {{/foreach}}
    {{/get}}
    {{#if @member}}
      <!-- Display link to premium membership if user is logged in -->
      <li><a href="/premium-membership">Premium Membership</a></li>
    {{else}}
      <!-- Display signup and signin links if user is not logged in -->
      <li><a href="/signup">Signup</a></li>
      <li><a href="/signin">Signin</a></li>
    {{/if}}
  </ul>
</nav>